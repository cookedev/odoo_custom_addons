<odoo>
    <data>

        <template id="carouselwithumbnails" name="Carousel With Thumbnails">
            <section class="o_colored_level o_half_screen_height text-center" style="background-image: none;">
                <div class="container">
                    
<!-- -->                        
                    <div class="row align-items-center">
                        <div class="pt16 pb16 col-lg-4 offset-lg-1 o_colored_level">
                            <h3 class="o_default_snippet_text">Unique experiences to drive engagement</h3>
                            <p class="o_default_snippet_text">
                                Users are looking to consume engaging content.<br/>We empowers our teams to create the most relevant content.
                            </p>
                        </div>
                        <div class="pt16 pb16 col-lg-5 offset-lg-1 o_colored_level">
                            <div id="slider">
                                <div id="carouselWithumbnails" class="carousel slide shadow">
                                <!-- main slider carousel items -->
                                    <div class="carousel-inner">
                                        <div class="active carousel-item" data-slide-number="0">
                                            <img src="navigo_snippets/static/img/sample1.jpg" class="img-fluid"/>
                                        </div>
                                        <div class="carousel-item" data-slide-number="1">
                                            <img src="navigo_snippets/static/img/sample2.jpg" class="img-fluid"/>
                                        </div>
                                        <div class="carousel-item" data-slide-number="2">
                                            <img src="navigo_snippets/static/img/sample3.jpg" class="img-fluid"/>
                                        </div>
                                        <div class="carousel-item" data-slide-number="3">
                                            <img src="navigo_snippets/static/img/sample4.jpg" class="img-fluid"/>
                                        </div>
                                        <div class="carousel-item" data-slide-number="4">
                                            <img src="navigo_snippets/static/img/sample5.jpg" class="img-fluid"/>
                                        </div>
                                        <div class="carousel-item" data-slide-number="5">
                                            <img src="navigo_snippets/static/img/sample6.jpg" class="img-fluid"/>
                                        </div>
                                        <div class="carousel-item" data-slide-number="6">
                                            <img src="navigo_snippets/static/img/sample7.jpg" class="img-fluid"/>
                                        </div>

                                        <a class="carousel-control-prev" href="#carouselWithumbnails" role="button" data-slide="prev">
                                            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                                            <span class="sr-only">Previous</span>
                                        </a>
                                        <a class="carousel-control-next" href="#carouselWithumbnails" role="button" data-slide="next">
                                            <span class="carousel-control-next-icon" aria-hidden="true"></span>
                                            <span class="sr-only">Next</span>
                                        </a>

                                    </div>
                                    <!-- main slider carousel nav controls -->

                                    <ul class="carousel-indicators list-inline mx-auto border px-2">
                                        <li class="list-inline-item active">
                                            <a id="carousel-selector-0" class="selected" data-slide-to="0" data-target="#carouselWithumbnails">
                                                <img src="navigo_snippets/static/img/thumbnails/sample1_tn.jpg" class="img-fluid"/>
                                            </a>
                                        </li>
                                        <li class="list-inline-item">
                                            <a id="carousel-selector-1" data-slide-to="1" data-target="#carouselWithumbnails">
                                                <img src="navigo_snippets/static/img/thumbnails/sample2_tn.jpg" class="img-fluid"/>
                                            </a>
                                        </li>
                                        <li class="list-inline-item">
                                            <a id="carousel-selector-2" data-slide-to="2" data-target="#carouselWithumbnails">
                                                <img src="navigo_snippets/static/img/thumbnails/sample3_tn.jpg" class="img-fluid"/>
                                            </a>
                                        </li>
                                        <li class="list-inline-item">
                                            <a id="carousel-selector-3" data-slide-to="3" data-target="#carouselWithumbnails">
                                                <img src="navigo_snippets/static/img/thumbnails/sample4_tn.jpg" class="img-fluid"/>
                                            </a>
                                        </li>
                                        <li class="list-inline-item">
                                            <a id="carousel-selector-4" data-slide-to="4" data-target="#carouselWithumbnails">
                                                <img src="navigo_snippets/static/img/thumbnails/sample5_tn.jpg" class="img-fluid"/>
                                            </a>
                                        </li>
                                        <li class="list-inline-item">
                                            <a id="carousel-selector-5" data-slide-to="5" data-target="#carouselWithumbnails">
                                                <img src="navigo_snippets/static/img/thumbnails/sample6_tn.jpg" class="img-fluid"/>
                                            </a>
                                        </li>
                                        <li class="list-inline-item">
                                            <a id="carousel-selector-6" data-slide-to="6" data-target="#carouselWithumbnails">
                                                <img src="navigo_snippets/static/img/thumbnails/sample7_tn.jpg" class="img-fluid"/>
                                            </a>
                                        </li>                    
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>
                <!--/main slider carousel-->
<!-- -->
                    
                </div>
            </section>
        </template>
<!--
        <template id="assets_frontend" inherit_id="website.assets_frontend" name="Navigo Website Snippets Assets">
            <xpath expr="link[last()]" position="after">
                <link rel="stylesheet" type="text/scss" href="/navigo_snippets/static/src/scss/main.scss"/>
            </xpath>
        </template>
-->
        <template id="navigo_website_snippets" inherit_id="website.snippets" name="Navigo Website Snippets">
            <xpath expr="//div[@id='snippet_feature']/div[@class='o_panel_body']" position="inside">
                <t t-snippet="navigo_snippets.carouselwithumbnails" t-thumbnail="/navigo_snippets/static/img/logo.png"/>
            </xpath>
        </template>

    </data>
</odoo>